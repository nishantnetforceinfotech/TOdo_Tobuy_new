<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.netforceinfotech.todo_tobuy.DashBoard.navigation.Navigation_items.Contact_us">

<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent">
        <!-- TODO: Update blank fragment layout -->

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
     android:weightSum="10"
        android:layout_alignParentStart="true">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="3">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_weight="2"
                app:srcCompat="@drawable/phone_icon"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                android:scaleType="fitCenter"
                android:id="@+id/imageView48"
                android:layout_gravity="center_horizontal" />

            <TextView
                android:text="Contact Us"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:textColor="#33a109"
                android:gravity="center_horizontal"
                android:layout_gravity="center_horizontal"
                android:textSize="24sp"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                android:layout_weight="2"
                android:id="@+id/textView26" />
        </LinearLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_margin="10dp"
            android:layout_weight="1.5"
            android:background="@drawable/edittext_bg"
            android:gravity="center_horizontal"
            android:layout_height="0dp">

            <EditText
                android:id="@+id/username"
                android:layout_width="match_parent"
                android:layout_weight="1.5"
                android:backgroundTint="@color/transparent"
                android:ems="10"
                android:hint="Email Address"
                android:inputType="textEmailAddress"
                android:textColorHint="#AE8F50"
                android:layout_alignParentStart="true"
                android:layout_height="match_parent"
                android:layout_marginLeft="5dp" />
      </RelativeLayout>


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_margin="10dp"
            android:layout_weight="1.5"
            android:background="@drawable/edittext_bg"
            android:gravity="center_horizontal">

            <Spinner
                android:id="@+id/spincontactus_sub"
                android:layout_width="match_parent"
                android:layout_height="match_parent"

                android:layout_margin="5dp"
                android:layout_marginLeft="5dp"
                android:background="#ffffff"
                android:gravity="left" />

            <ImageView
                android:id="@+id/imageView4__"
                android:layout_width="10dp"
                android:layout_height="10dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="20dp"
                android:src="@drawable/spinner_down_icon" />
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:layout_weight="2"
            android:background="@drawable/edittext_bg"
            android:gravity="center_horizontal">

            <EditText
                android:id="@+id/desription"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginLeft="5dp"
                android:backgroundTint="@color/transparent"
                android:ems="10"
                android:hint="Description"
                android:inputType="textEmailAddress"
                android:textColorHint="#AE8F50"
                android:layout_alignParentStart="true" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"

            android:layout_height="0dp"
            android:layout_margin="10dp"
            android:background="#FF33A109"
            android:layout_weight="1.5">

            <TextView
                android:text="SUBMIT"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true"
                android:textColor="#ffffff"
                android:id="@+id/textView27" />
        </RelativeLayout>


    </LinearLayout>


</RelativeLayout>

</FrameLayout>
