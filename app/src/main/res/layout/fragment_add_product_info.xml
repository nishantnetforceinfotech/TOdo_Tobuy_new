<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.netforceinfotech.todo_tobuy.DashBoard.To_Buy_Group_Fragment.Group_Info.Group_checked_adapter.New_group_description.Add_product_info">

    <!-- TODO: Update blank fragment layout -->

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="left|top">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <LinearLayout
                android:orientation="vertical"
                android:weightSum="10"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_weight="2"
                    android:weightSum="2"
                    android:layout_height="0dp" >

                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:id="@+id/relativeLayout4"
                        android:background="@drawable/gallarybgbox_icon"
                        android:layout_height="match_parent" >

                        <ImageView
                            android:layout_width="100dp"
                            android:layout_height="100dp"
                            android:layout_centerHorizontal="true"
                            android:src="@drawable/gallarybox_icon"
                            android:scaleType="fitCenter"
                            android:layout_centerVertical="true"
                            android:id="@+id/imageView32" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="match_parent" >

                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:weightSum="2"
                            android:layout_height="match_parent" >

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_weight="1"
                                android:layout_height="match_parent" >

                                <Button
                                    style="?android:attr/buttonStyleSmall"
                                    android:layout_width="100dp"
                                    android:layout_height="20dp"
                                    android:background="@drawable/group_button"
                                    android:layout_margin="10dp"
                                    android:id="@+id/button16"
                                    android:layout_centerVertical="true"
                                    android:layout_centerHorizontal="true" />
                            </RelativeLayout>

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_weight="1"
                                android:layout_height="match_parent" >

                                <EditText
                                    style="?android:attr/buttonStyleSmall"
                                    android:layout_width="100dp"
                                    android:layout_height="20dp"
                                    android:text="Vegetables"
                                    android:singleLine="true"
                                    android:textAllCaps="false"
                                    android:layout_margin="10dp"
                                    android:background="@drawable/vegetabletext_box"
                                    android:id="@+id/editText12"
                                    android:textColor="#32A007"
                                    android:textSize="10sp"
                                    android:layout_centerVertical="true"
                                    android:layout_centerHorizontal="true" />
                            </RelativeLayout>
                        </LinearLayout>
                    </RelativeLayout>
                </LinearLayout>

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_weight="6"
                    android:weightSum="6"
                    android:layout_height="0dp" >

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_weight="1"
                        android:weightSum="2"

                        android:layout_height="0dp"
                        android:layout_margin="10dp"
                         >

                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:background="@drawable/input_newitem"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:layout_height="match_parent">
                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="40dp"
                                android:textSize="12sp"
                                android:gravity="center_vertical|left"

                                android:background="@color/transparent"
                                android:layout_alignParentRight="true"
                                android:layout_alignParentEnd="true"
                                android:layout_alignParentLeft="true"
                                android:layout_alignParentStart="true"
                                android:id="@+id/editText3"
                                android:hint=" Name:"
                                android:layout_marginLeft="5dp"
                                android:textColorHint="#32A007"
                                android:textColor="#32A007"
                                android:layout_alignParentBottom="true"
                                android:layout_alignParentTop="true" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_marginRight="5dp"
                            android:layout_marginLeft="5dp"
                            android:background="@drawable/input_newitem"
                            android:layout_height="match_parent">

                            <EditText
                                android:layout_width="match_parent"
                                android:layout_height="40dp"
                                android:textSize="12sp"
                                android:gravity="center_vertical|center_horizontal"
                                android:background="@color/transparent"
                                android:id="@+id/editText13"

                                android:textColor="#32A007"
                                android:layout_alignParentLeft="true"
                                android:layout_alignParentStart="true"
                                android:layout_alignParentTop="true"
                                android:layout_alignParentBottom="true" />
                        </RelativeLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_weight="1"
                        android:weightSum="2"
                        android:layout_margin="10dp"
                        android:layout_height="0dp"
                        >

                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:background="@drawable/input_newitem"
                            android:layout_height="match_parent" >

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="40dp"
                                android:textSize="12sp"
                                android:gravity="center_vertical|left"
                                android:background="@color/transparent"
                                android:layout_alignParentRight="true"
                                android:layout_alignParentEnd="true"
                                android:layout_alignParentLeft="true"
                                android:layout_alignParentStart="true"
                                android:id="@+id/editText14"
                                android:hint=" Brand:"
                                android:layout_marginLeft="10dp"
                                android:textColorHint="#32A007"
                                android:textColor="#32A007"
                                android:layout_alignParentBottom="true"
                                android:layout_alignParentTop="true" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_marginRight="5dp"
                            android:layout_marginLeft="5dp"
                            android:background="@drawable/input_newitem"
                            android:layout_height="match_parent" >

                            <EditText
                                android:layout_width="match_parent"
                                android:layout_height="40dp"
                                android:textSize="12sp"
                                android:gravity="center_vertical|center_horizontal"
                                android:background="@color/transparent"
                                android:id="@+id/editText15"
                                android:textColor="#32A007"
                                android:layout_alignParentLeft="true"
                                android:layout_alignParentStart="true"
                                android:layout_alignParentTop="true"
                                android:layout_alignParentBottom="true" />
                        </RelativeLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"

                        android:layout_weight="1"
                        android:weightSum="2"
                        android:layout_margin="10dp"
                        android:layout_height="0dp">

                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:background="@drawable/input_newitem"
                            android:layout_height="match_parent" >

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="40dp"
                                android:textSize="12sp"
                                android:gravity="center_vertical|left"
                                android:background="@color/transparent"
                                android:layout_alignParentRight="true"
                                android:layout_alignParentEnd="true"
                                android:layout_alignParentLeft="true"
                                android:layout_alignParentStart="true"
                                android:id="@+id/editText16"
                                android:hint=" Alternatives:"
                                android:layout_marginLeft="10dp"
                                android:textColorHint="#32A007"
                                android:textColor="#32A007"
                                android:layout_alignParentBottom="true"
                                android:layout_alignParentTop="true" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_marginRight="5dp"
                            android:layout_marginLeft="5dp"
                            android:background="@drawable/input_newitem"
                            android:layout_height="match_parent" >

                            <EditText
                                android:layout_width="match_parent"
                                android:layout_height="40dp"
                                android:textSize="12sp"
                                android:gravity="center_vertical|center_horizontal"
                                android:background="@color/transparent"
                                android:id="@+id/editText17"
                                android:textColor="#32A007"
                                android:layout_alignParentLeft="true"
                                android:layout_alignParentStart="true"
                                android:layout_alignParentTop="true"
                                android:layout_alignParentBottom="true" />
                        </RelativeLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_weight="1"
                        android:weightSum="2"
                        android:layout_margin="10dp"
                        android:layout_height="0dp"
                       >

                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:background="@drawable/input_newitem"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:layout_height="match_parent" >

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="40dp"
                                android:textSize="12sp"
                                android:gravity="center_vertical|left"
                                android:background="@color/transparent"
                                android:layout_alignParentRight="true"
                                android:layout_alignParentEnd="true"
                                android:layout_alignParentLeft="true"
                                android:layout_alignParentStart="true"
                                android:id="@+id/editText18"
                                android:hint=" Store:"
                                android:layout_marginLeft="10dp"
                                android:textColorHint="#32A007"
                                android:textColor="#32A007"
                                android:layout_alignParentBottom="true"
                                android:layout_alignParentTop="true" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_marginRight="5dp"
                            android:layout_marginLeft="5dp"
                            android:background="@drawable/input_newitem"
                            android:layout_height="match_parent" >

                            <EditText
                                android:layout_width="match_parent"
                                android:layout_height="40dp"
                                android:textSize="12sp"
                                android:gravity="center_vertical|center_horizontal"
                                android:background="@color/transparent"
                                android:id="@+id/editText19"
                                android:textColor="#32A007"
                                android:layout_alignParentLeft="true"
                                android:layout_alignParentStart="true"
                                android:layout_alignParentTop="true"
                                android:layout_alignParentBottom="true" />
                        </RelativeLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"

                        android:layout_weight="2"
                        android:weightSum="2"
                        android:layout_margin="10dp"
                        android:layout_height="0dp">

                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:background="@drawable/input_newitem"
                            android:layout_height="40dp" >

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="40dp"
                                android:textSize="12sp"
                                android:gravity="center_vertical|left"
                                android:background="@color/transparent"
                                android:layout_alignParentRight="true"
                                android:layout_alignParentEnd="true"
                                android:layout_alignParentLeft="true"
                                android:layout_alignParentStart="true"
                                android:id="@+id/editText20"
                                android:hint=" Comments:"
                                android:layout_marginLeft="10dp"
                                android:textColorHint="#32A007"
                                android:textColor="#32A007"

                                android:layout_alignParentTop="true" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_marginRight="5dp"
                            android:layout_marginLeft="5dp"
                            android:background="@drawable/input_newitem"
                            android:layout_height="match_parent" >

                            <EditText
                                android:layout_width="match_parent"
                                android:layout_height="40dp"
                                android:textSize="12sp"
                                android:gravity="center_vertical|center_horizontal"
                                android:background="@color/transparent"
                                android:id="@+id/editText21"
                                android:textColor="#32A007"
                                android:layout_alignParentLeft="true"
                                android:layout_alignParentStart="true"
                                android:layout_alignParentTop="true"
                                android:layout_alignParentBottom="true" />
                        </RelativeLayout>
                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_weight="1"
                    android:layout_margin="5dp"
                    android:layout_width="match_parent"
                    android:layout_height="0dp" >

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_weight="1"
                        android:layout_height="match_parent" >

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="40dp"
                            android:id="@+id/imageView35"
                            android:layout_margin="10dp"
                            android:src="@drawable/done_button"
                            android:layout_centerVertical="true"
                            android:layout_centerHorizontal="true" />
                    </RelativeLayout>

                </LinearLayout>
            </LinearLayout>
        </RelativeLayout>
    </RelativeLayout>

</FrameLayout>
